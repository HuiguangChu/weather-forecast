{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es2016",
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}
